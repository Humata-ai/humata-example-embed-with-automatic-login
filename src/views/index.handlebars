<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <iframe
      style="border: 1px solid grey"
      frameborder="0"
      width="100%"
      height="600"
      allow="microphone; clipboard-write"
    ></iframe>
    <script>
      const shareLink = "<SHARE_LINK>"; const iframe =
      document.querySelector("iframe"); iframe.onload = () => { setTimeout(() =>
      { iframe.contentWindow.postMessage( { token: "{{token}}" },
      "https://app.humata.ai", ); }, 2000); }; iframe.src = shareLink;
    </script>
  </body>
</html>